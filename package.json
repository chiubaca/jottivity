{
  "name": "jottivity-v2",
  "version": "1.0.0",
  "description": "Jottivity rewrite",
  "author": "Alex Chiu",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "generate": "tsc -p functions/tsconfig.json && nuxt-ts generate",
    "start": "nuxt-ts start",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest",
    "test:db":"npm test -- ./firebase/test-rules.test.ts --config ./firebase/jest.config.js",
    "ndev": "concurrently \"netlify dev\" \"tsc -p functions/tsconfig.json -w\" ",
    "ndep": "npm run generate && netlify deploy --open",
    "cfunc": "tsc -p functions/tsconfig.json"
  },
  "dependencies": {
    "nuxt": "^2.0.0",
    "@nuxt/typescript-runtime": "^0.4.0",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/pwa": "^3.0.0-0",
    "@nuxtjs/dotenv": "^1.4.0",
    "firebase": "^7.15.0"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^0.6.0",
    "@nuxtjs/eslint-config-typescript": "^1.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/stylelint-module": "^3.1.0",
    "@types/aws-lambda": "^8.10.53",
    "@types/jest": "^26.0.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "concurrently": "^5.2.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.0.0",
    "lambda-tester": "4.0.1",
    "netlify-cli": "^2.51.0",
    "prettier": "^1.19.1",
    "stylelint": "^10.1.0",
    "ts-jest": "^25.0.0",
    "vue-jest": "^4.0.0-0",
    "vuex-module-decorators": "^0.17.0",
    "@firebase/testing": "^0.20.5"
  }
}
